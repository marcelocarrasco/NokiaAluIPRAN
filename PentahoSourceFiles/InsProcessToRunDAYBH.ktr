    <sql>MERGE INTO PROCESS_TO_RUN PRO&#xa;USING &#x28;SELECT &#x27;&#x24;&#x7b;SUM-IPRAN-DAY-BH&#x7d;&#x27;           																			AS  PROCESS_NAME,&#xa;              &#x27;&#x24;&#x7b;PARAM-SUM-IPRAN-DAY-BH&#x7d;&#x27;&#x7c;&#x7c;TO_CHAR&#x28;TO_DATE&#x28;SUBSTR&#x28;&#x27;&#x24;&#x7b;FECHA_PROC&#x7d;&#x27;,1,10&#x29;,&#x27;DD.MM.YYYY&#x27;&#x29;&#x2b;1,&#x27;DD.MM.YYYY&#x27;&#x29;	AS  PARAMS,&#xa;              TO_CHAR&#x28;TO_DATE&#x28;SUBSTR&#x28;&#x27;&#x24;&#x7b;FECHA_PROC&#x7d;&#x27;,1,10&#x29;,&#x27;DD.MM.YYYY&#x27;&#x29;&#x2b;1,&#x27;DD.MM.YYYY&#x27;&#x29;								AS	FECHA_TO_RUN,&#xa;              &#x27;Daily&#x27;                         																			AS  TIPO,&#xa;              &#x27;&#x24;&#x7b;GRUPO&#x7d;&#x27;                      																			AS  GRUPO,&#xa;              1                               																			AS  STATUS&#xa;      FROM  DUAL&#x29; DATOS&#xa;ON  &#x28;PRO.PROCESS_NAME &#x3d; DATOS.PROCESS_NAME AND PRO.FECHA_TO_RUN &#x3d; DATOS.FECHA_TO_RUN&#xa;     AND PRO.STATUS &#x3d; DATOS.STATUS AND PRO.GRUPO &#x3d; DATOS.GRUPO AND PRO.PROCESADO IS NULL&#x29;      &#xa;WHEN NOT MATCHED THEN&#xa;  INSERT &#x28;PROCESS_NAME,PARAMS,FECHA_TO_RUN,TIPO,GRUPO,STATUS&#x29;&#xa;  VALUES &#x28;DATOS.PROCESS_NAME,DATOS.PARAMS,DATOS.FECHA_TO_RUN,DATOS.TIPO,DATOS.GRUPO,DATOS.STATUS&#x29;</sql>
    <set_params>N</set_params>
